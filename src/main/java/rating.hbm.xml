<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package = "com.jkmsteam.model.dto">
<class name="Rating" table="ratings">
<id name="id" column="ID" type="int">
<generator class="increment"></generator>
</id>
<property name="userId" column="user_id" type="int"></property>
<property name="placeId" column="place_id" type="string"></property>
<property name="dead" column="dead" type="int"></property>
<property name="justRight" column="just_right" type="boolean"></property>
<property name="jumping" column="jumping" type="boolean"></property>
<property name="coverCharge" column="cover_charge" type="boolean"></property>
<property name="crowded" column="crowded" type="boolean"></property>
<property name="expensive" column="expensive" type="boolean"></property>
<property name="loud" column="loud" type="boolean"></property>
<property name="bigGroups" column="big_groups" type="boolean"></property>
<property name="smallGroups" column="small_groups" type="boolean"></property>
<property name="safePlace" column="safe_place" type="boolean"></property>
<property name="goodParking" column="good_parking" type="boolean"></property>
</class>
</hibernate-mapping> 
